  },
  "homepage": "https://github.com/ngireric123/QuickCredit#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "coveralls": "^3.0.3",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
