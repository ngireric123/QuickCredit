    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "pg": "^7.11.0",
    "pg-promise": "^8.7.2",
    "swagger-ui-express": "^4.0.4",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
